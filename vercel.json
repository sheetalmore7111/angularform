{
    "version": 2,
    "builds": [
      { "src": "dist/angular-vercel-json-server", "use": "@vercel/static-build" },
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/$1" },
      { "src": "/(.*)", "dest": "/dist/angular-vercel-json-server/$1" }
    ]
  }
  